$TTL	43200 ; 24 hours could have been written as 24h or 1d
$ORIGIN ja.tc.
@  1D  IN  SOA ns2.ja.tc. hostmaster.ja.tc. (
			      2015100704 ; serial
			      21600 ; refresh
			      10800 ; retry
			      604800 ; expire
			      3600 ; minimum
			     )
       IN  NS     ns2.ja.tc. ; in the domain
       IN  NS     ns3.ja.tc. ; in the domain
       IN  NS     ns4.ja.tc. ; in the domain
       IN  NS     ns3.nsentry.de. ; external to domain
       IN  NS     ns4.nsentry.de. ; external to domain	   
       IN  MX  10 mail.ja.tc. ; external mail provider
	   
